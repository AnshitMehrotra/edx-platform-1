<label class="account-settings-field-title">
    <%- gettext(title) %>
</label>
<select name="select" class="account-settings-field-value">
<% _.each(select_options, function(select_option) { %>
    <option value="<%- select_option[0] %>"><%- select_option[1] %></option>
<% }); %>
</select>
<small class="account-settings-field-message">
    <%- gettext(message) %>
</small>
<hr class="divider">
